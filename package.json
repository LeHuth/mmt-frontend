{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --trace-warnings",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "yarn docker:up && yarn prisma migrate deploy && vitest",
    "ci:test": "yarn prisma migrate deploy && vitest",
    "prisma:generate": "dotenv -e .env -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate deploy --name prod"
  },
  "engines": {
    "node": "16.14.0"
  },
  "devDependencies": {
    "@nuxt/content": "^2.2.1",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/test-utils-edge": "3.0.1-rc.0-27810184.d991a55",
    "@nuxtjs/color-mode": "^3.1.8",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vueuse/nuxt": "^10.1.2",
    "eslint": "^8.39.0",
    "nuxt": "^3.4.2",
    "nuxt-icon": "^0.1.7",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.3.2",
    "vitest": "^0.24.5"
  },
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.3",
    "@nuxt/types": "^2.16.3",
    "@nuxtjs/tailwindcss": "^6.6.7",
    "@pinia/nuxt": "^0.4.9",
    "@vueuse/core": "^10.1.2",
    "daisyui": "^2.51.6",
    "jose": "^4.14.3",
    "pinia": "^2.0.35",
    "typescript": "^5.0.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "stripe": "^10.15.0",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  }
}
